<div id="openModal" class="modalDialog">
    <div>
        <a href="#close" title="Close" class="close">X</a>
        <h4>iSign mobilus parašas</h4>
        <form>
            <div>
                <div class="form-group">
                    <input id="phone" class="form-control" placeholder="Tel. Nr." />
                </div>
                <div class="form-group">
                    <input id="code" class="form-control" placeholder="Asmens kodas" />
                </div>
            </div>
            <button class="btn btn-block btn-primary" onclick="(function () { window.location = 'http://localhost:61392/api/BankView/LoginMobile/clientId/' + document.getElementById('phone').value + '/' + document.getElementById('code').value; return false })(); return false" type="submit">Tęsti</button>
        </form>
    </div>
</div>